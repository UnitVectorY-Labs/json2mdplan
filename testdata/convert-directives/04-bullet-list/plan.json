{
  "directives": [
    {
      "op": "if_present",
      "value": {
        "from": "root",
        "path": "/highlights"
      },
      "when": {
        "test": "non_empty",
        "value": {
          "from": "root",
          "path": "/highlights"
        }
      }
    },
    {
      "level_from_base": 0,
      "op": "heading",
      "text": {
        "kind": "schema_title",
        "schema_title": {
          "fallback": "Highlights",
          "path": "/highlights"
        }
      }
    },
    {
      "items": {
        "from": "root",
        "path": "/highlights"
      },
      "op": "bullet_list",
      "skip_empty": true,
      "when": {
        "test": "non_empty",
        "value": {
          "from": "root",
          "path": "/highlights"
        }
      }
    }
  ],
  "settings": {
    "base_heading_level": 1
  },
  "version": 1
}